<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Server.model.DB.FilmCategoryFilmEntity" table="Film_CategoryFilm" schema="dbo"
           catalog="ProjectMusicFilm">
        <id name="id">
            <column name="id" sql-type="bigint"/>
            <generator class="identity"></generator>
        </id>
        <property name="filmId">
            <column name="filmId" sql-type="bigint" not-null="true"/>
        </property>
        <property name="categoryId">
            <column name="categoryId" sql-type="bigint" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>