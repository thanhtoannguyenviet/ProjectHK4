<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Server.model.DB.SeriFilmEntity" table="SeriFilm" schema="dbo" catalog="ProjectMusicFilm">
        <id name="id">
            <column name="id" sql-type="bigint"/>
            <generator class="identity"></generator>
        </id>
        <property name="listFilm">
            <column name="listFilm" sql-type="text" not-null="true"/>
        </property>
        <property name="ext">
            <column name="ext" sql-type="text" not-null="true"/>
        </property>
        <property name="index">
            <column name="index" sql-type="int" not-null="true"/>
        </property>
        <property name="range">
            <column name="range" sql-type="int" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>